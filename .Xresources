xft*dpi: 96
xft*antialias: 1
Xft*autohint: 0
xft*hinting: 1
xft*rgba: rgb
xft*hintstyle: hintfull
xft*lcdfilter: lcddefault

urxvt*perl-ext-common: default,tabbedex,clipboard,url-select,keyboard-select
urxvt*iconFile: /usr/share/icons/oxygen/32x32/apps/yakuake.png

urxvt*keysym.F11: perl:fullscreen:switch
urxvt*font: xft:Meslo LG S for Powerline:pixelsize=11,xft:PowerlineSymbols:pixelsize=11
urxvt*boldFont: xft:Meslo LG S for Powerline:pixelsize=11,xft:PowerlineSymbols:bold:pixelsize=11
urxvt*cursorBlink: 1
urxvt*cursorUnderline: 1
urxvt*urlLauncher: /usr/bin/google-chrome-stable
urxvt*matcher.button: 1
urxvt*underlineURLs: 1
urxvt*colorUL: #4682B4
urxvt*scrollBar_right: 1
urxvt*scrollstyle: plain
urxvt*scrollTtyOutput: 0
urxvt*scrollWithBuffer: 1
urxvt*scrollTtyKeypress: 1
urxvt*secondaryScroll: 1
urxvt*depth: 32
urxvt*geometry: 82x26
urxvt*copyCommand: xclip -i -selection clipboard
urxvt*pasteCommand: xclip -o -selection clipboard
urxvt*loginShell: 1

URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 7
URxvt.tabbed.tab-bg: 0
URxvt.termName: rxvt-unicode-256color
URxvt.urgentOnBell: 1
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: 1

URxvt.iso14755: 0
URxvt.iso14755_52: 0
URxvt.keysym.C-Up: perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.keysym.C-S-Up: perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

URxvt*background: #2B2B2B
URxvt*foreground: #DEDEDE

URxvt*colorUL: #86a2b0

! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC
